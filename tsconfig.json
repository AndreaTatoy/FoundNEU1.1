    "module": "commonjs",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@react-navigation/native", "@react-navigation/stack", "@react-navigation/bottom-tabs", "@react-native-community/datetimepicker", "react-native"]
  }
}
